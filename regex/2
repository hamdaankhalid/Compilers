#include <iostream>
#include <optional>

enum TokenType {
 LITERAL,
 WILDCARD,
 DIGIT,
 NOT_DIGIT,
 ALPHABET,
 NOT_ALPHABET,
 WHITESPACE,
 NOT_WHITESPACE,
 SYMBOL,
 IN_RANGE,
 NOT_IN_RANGE,
 ZERO_OR_MORE,
 ONE_OR_MORE,
 ZERO_OR_ONE,
 START_OF_STR, // single line
 END_OF_STR, // single line
 };




int main() { 
	const std::string phoneNumExpr = "\d{3}\-\d{3}-\d{4}";
	const std::string emailExpr = "(\d|\w@(\d|\w)\.com";

	std::cout << "la la lala" << std::endl; 
}
